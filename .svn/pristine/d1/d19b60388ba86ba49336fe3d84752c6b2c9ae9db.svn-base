@CHARSET "UTF-8";

.editor{
	position:relative;
}
.login1{
	position:absolute;
	z-index:9999;
	line-height:30px;
	top:50%;
	margin-top:-15px;
	text-align:center;
}

.mask{
	width:100%;
	height:100%;
	background-color:#eee;
	z-index:100;
	position:absolute;
	top:0px;
	left:0px;
	opacity:.2;
	filter:alpha(opacity=20);
}