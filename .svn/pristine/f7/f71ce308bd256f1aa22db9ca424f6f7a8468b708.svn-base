	width: 280px;
	margin: 5px auto;
}

.hotLabelInfo {
	overflow: hidden;
	text-align: center;
}

.hotLabelInfo tr {
	height: 20px;
}

.hotLabelInfo span {
	width: 15px;
	height: 15px;
	display: block;
	margin-right: 10px;
