}

.hotLabelInfo span.rank2 {
	background: url(../../img/bg_2.png) no-repeat;
}

.hotLabelInfo span.rank3 {
	background: url(../../img/bg_3.png) no-repeat;
}

.hotLabelInfo span.rank4 {
	background: url(../../img/bg_4.png) no-repeat;
}

/*interest*/
.interest {
	width: 735px;
	height: 200px;
	float: right;
}

.interestTitle {
	background: url(../../img/Blog/bg_user_xingqu.png) no-repeat;
	width: 735px;
	height: 40px;
}

.interestTitle h3 {
	color: #fff;
	line-height: 35px;
	margin-left: 40px;
	float: left;
}

.interestContent {
	background: #fff;
	height: 160px;
	overflow: hidden;
}

.interestTitle a {
	color: #fff;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 14px;
	font-weight: bold;
}

.interest_blog a {
	float: left;
	width: 80px;
}

.interest_forum a {
	margin-left: 20px;
	float: left;
	width: 80px;
}

.interest_question a {
	margin-left: 20px;
	float: left;
	width: 70px;
}

.interest_label a {
	margin-left: 10px;
	float: left;
	width: 60px;
}

.interest_blog_Info {
	width: 720px;
	margin: 5px;
	display: none;
}

.interest_block {
	display: block;
}

.interest_Info_right {
	width: 120px;
	float: right;
	margin-right: 15px;
}

.interest_blog_Info_left {
	width: 580px;
	float: left;
}

.interest_blog_Info_left_Info {
	overflow: hidden;
	text-align: center;
	table-layout: fixed;
}

.interest_blog_Info_left_Info tr {
	height: 20px;
}

.interest_blog_Info_left_Info td {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis; /*截断文字，显示省略号(...)*/
	-o-text-overflow: ellipsis; /*Opera的专用截断文字的属性*/
}

.interest_blog_Info_left_Info a {
	color: blue;
}

.interest_blog_Info_left_Info a:hover {
	color: red;
}

/*floor3*/
.floor3 {
	clear: both;
	overflow: hidden;
}

.attach {
	width: 1050px;
	height: 300px;
	background: #fff;
}

.attachTitle {
	background: url(../../img/Blog/bg_user_attach.png) no-repeat;
	width: 1050px;
	height: 35px;
}

.attachTitle h3 {
	color: #fff;
	line-height: 35px;
	margin-left: 40px;
	float: left;
}

.attachContent {
	width: 1000px;
	margin: 10px auto;
}

.attachInfo {
	width: 1000px;
	table-layout: fixed;
}

.attachInfo tr {
	height: 22px;
}

.attachInfo th {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.attachInfo a {
	color: blue;
}

.attachInfo a:hover {
	color: red;
}

/*baidu*/
.clearfix:after {
	content: '\20';
	display: block;
	height: 0;
	clear: both
}

.wordwrap {
	word-break: break-all;
	word-wrap: break-word
}

pre.wordwrap {
	white-space: pre-wrap
}

.yahei {
	font-family: "microsoft yahei", "\5FAE\8F6F\96C5\9ED1", Tahoma, Arial,
		Helvetica, STHeiti
}

.dotted-bottom {
	border-bottom: dotted 1px #ddd
}

.displaynone {
	display: none
}

html {
	overflow-y: scroll
}

#wrapper {
	width: 100%;
	position: relative;
	overflow: hidden
}

#head {
	width: 100%
}

#content {
	box-shadow: 0 1px 3px 2px rgba(0, 0, 0, .2);
	-moz-box-shadow: 0 1px 3px 2px rgba(0, 0, 0, .2);
	-webkit-box-shadow: 0 1px 3px 2px rgba(0, 0, 0, .2)
}

#content, #foot {
	width: 740px;
	margin: 0 auto
}

.songti {
	font-family: \5b8b\4f53
}

