}

/*Floor2*/
.floor2 {
	clear: both;
	overflow: hidden;
}
/*活跃标签*/
.hotLabel {
	margin-bottom: 10px;
	width: 285px;
	background: #FFF;
	height: 200px;
	float: left;
}

.hotLabelTitle {
	background: url(../../img/Blog/bg_Index_dongtai.png) no-repeat;
	width: 285px;
	height: 35px;
}

.hotLabelTitle h3 {
	color: #fff;
	line-height: 35px;
	margin-left: 40px;
	float: left;
}

