<script type="text/javascript">

  
      function check()
      {
        var reason=$.trim(document.getElementById("reason").value);
        if(reason.length<=0)
          {
            alert("Please fill out the reason");
            return false;
          }else if(reason.lengtn>200)
          {
            alert("Count exceeds the limit");
            return false;
          }else{
            return true;
          }
          
          

      }
	
</script>


