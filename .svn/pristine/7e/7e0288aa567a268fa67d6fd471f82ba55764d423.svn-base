$(document).ready(function() {
	
	$('#newQ').click(function(){
		$('#listNew').css("display","block");
		$('#listDoQ').css("display","none");
		$('#listUnDoQ').css("display","none");
		
		$('#newQ').css("background", "#087ec4");
		$('#newQ a').css("color", "#fff");
		$('#doQ').css("background", "#fff");
		$('#doQ a').css("color", "#333");
		$('#undoQ').css("background", "#fff");
		$('#undoQ a').css("color", "#333");
	
	});
	
    $('#doQ').click(function(){
    	$('#listNew').css("display","none");
		$('#listDoQ').css("display","block");
		$('#listUnDoQ').css("display","none");
		
		$('#newQ').css("background", "#fff");
		$('#newQ a').css("color", "#333");
		$('#doQ').css("background", "#087ec4");
		$('#doQ a').css("color", "#fff");
		$('#undoQ').css("background", "#fff");
		$('#undoQ a').css("color", "#333");
		
	});

    $('#undoQ').click(function(){
    	$('#listNew').css("display","none");
		$('#listDoQ').css("display","none");
		$('#listUnDoQ').css("display","block");
		
		$('#newQ').css("background", "#fff");
		$('#doQ').css("background", "#fff");
		$('#undoQ').css("background", "#087ec4");
		$('#newQ a').css("color", "#333");
		$('#doQ a').css("color", "#333");
		$('#undoQ a').css("color", "#fff");
    });

	
});