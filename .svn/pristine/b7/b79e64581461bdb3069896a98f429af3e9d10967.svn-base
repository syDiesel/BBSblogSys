}
.nav_login a{
	color:#fff;
}
.login{
	background:url(../images/home/bg_login.png);
	width:75px;
	height:25px;
	border-radius:4px;
}
.register{
	background:url(../images/home/bg_register.png);
	width:75px;
	height:25px;
	border-radius:4px;
}
.nav_I18N {
	margin-left:20px;
}
.nav_I18N li{
	float:left;
	font-size:18px;	
	padding:13px 10px;
	
}
.nav_I18N  a{
	color:#FFF;	
}



/* 标题+搜索栏*/
.name_search{
	margin:10px auto;
	text-align:center;
	width:900px;
	height:120px;
	background:#FFF;
	position:relative;
}
.name_search h3{
	float:left;
	font-size:50px;
	margin:20px 40px;
	letter-spacing:8px;
}
.searchDiv {
	right:20px;
	bottom:0px;
	position:absolute; 
	
}
/*更多*/
.more{
	background:url(../images/home/bg_more.png) no-repeat;
	width:75px;
	height:25px;
	text-align:center;
	float:right;
	color:#FFF;
	margin:10px 10px;
	border-radius:2px;
}
.more a{
	font-size:14px;
	
}
.more a:hover{
	color:#fff;
}


/*主体*/
.mainbox{
	width:1200px;	
