@CHARSET "UTF-8";

*{
	font-family:"Microsoft YaHei", "New York", serif;
}
body{
	margin:0 auto;
	overflow:scroll;
	background:url(../images/home/bg_body.jpg);

}
/*header*/
.xiugai{
	width:100px;
	height:30px;
	
}

#level{
	color:blue;
}
