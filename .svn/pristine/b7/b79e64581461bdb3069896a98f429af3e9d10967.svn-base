	width：100px;
}
.top{
	text-align:center;
}
.nav{
	background:url(../images/home/bg_nav.jpg);
	height:50px;
	margin-bottom:10px;
}
.nav a:hover{
	color:#FFF;
}
.nav_btn {
	margin-left:80px;
} 
.nav_btn a{
	color:#fff;
}
.nav_btn li{
	float:left;
	font-size:18px;	
	padding:13px 20px;
}
.nav_btn li:hover{
	background:url(../images/home/bg_navbtn.png) repeat-x;
}
.nav_login{
	float:left;
	text-align:center;
	margin-left:70px;
	margin-top:15px;
